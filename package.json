{
  "name": "assetgraph-i18n",
  "description": "i18n plugin for assetgraph-builder",
  "repository": "git://github.com/One-com/assetgraph-builder.git",
  "version": "0.7.2",
  "main": "lib/index.js",
  "keywords": [
    "assetgraph",
    "localization",
    "internationalization",
    "i18n",
    "l10n"
  ],
  "maintainers": [
    {
      "name": "Andreas Lind",
      "email": "andreaslindpetersen@gmail.com"
    }
  ],
  "license": "BSD-3-Clause",
  "files": [
    "i18n.js",
    "lib",
    "bin"
  ],
  "bin": {
    "applyBabelJob": "bin/applyBabelJob.js",
    "checkLanguageKeys": "bin/checkLanguageKeys.js",
    "makeBabelJob": "bin/makeBabelJob.js"
  },
  "dependencies": {
    "esanimate": "^1.1.0",
    "escodegen": "^1.9.1",
    "esmangle": "^1.0.1",
    "esprima": "^4.0.0",
    "estraverse": "^4.2.0",
    "estraverse-fb": "^1.3.2",
    "lodash": "^4.16.1",
    "memoizesync": "0.5.0",
    "optimist": "0.6.1",
    "plurals-cldr": "^1.0.2",
    "urltools": "^0.2.1"
  },
  "devDependencies": {
    "assetgraph": "^5.1.1",
    "coveralls": "^3.0.2",
    "eslint": "3.19.0",
    "eslint-config-onelint": "^4.0.0",
    "eslint-config-pretty-standard": "1.3.0",
    "eslint-plugin-prettier": "2.6.0",
    "mocha": "^3.0.0",
    "nyc": "^8.3.0",
    "prettier": "1.11.1",
    "systemjs-builder": "0.15.32",
    "temp": "0.8.3",
    "unexpected": "^10.8.1"
  },
  "peerDependencies": {
    "assetgraph": "^4.0.0"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "npm run lint && mocha",
    "travis": "npm run lint && npm run coverage",
    "coverage": "NODE_ENV=development nyc --reporter=lcov --reporter=text --all -- mocha && echo google-chrome coverage/lcov-report/index.html"
  },
  "nyc": {
    "include": [
      "lib/**",
      "bin/**"
    ]
  }
}
